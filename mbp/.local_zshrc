#.local_zshrc

# for android path setting
export PATH=/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/Applications/eclipse/android/platform-tools

# for FBX path setting
FBXSDK_ROOT=/Applications/Autodesk/FBX\ SDK/2013.3/

# path setting
PATH=/usr/local/bin:$PATH
PATH=/usr/local/share/python:$PATH

## go lang
PATH=$GOROOT/bin:$PATH

# utility
LABO_ROOT=~/labo
LABO_GFX_ROOT=$LABO_ROOT/graphic


alias vim='env LANG=ja_JP.UTF-8 /Applications/MacVim.app/Contents/MacOS/Vim "$@"'
alias updatedb='sudo /usr/libexec/locate.updatedb'
alias chrome='open -a Google\ Chrome'
alias xcode='open -a /Applications/Xcode.app/Contents/MacOS/Xcode'

alias -s xcodeproj=xcode
